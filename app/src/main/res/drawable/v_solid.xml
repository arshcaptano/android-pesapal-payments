<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <stroke android:width="1dp" android:color="@color/cm0" />
            <solid android:width="1dp" android:color="@color/cm0" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <stroke android:width="1dp" android:color="@color/cm1" />
            <solid android:width="1dp" android:color="@color/cm1" />
        </shape>
    </item>
</selector>